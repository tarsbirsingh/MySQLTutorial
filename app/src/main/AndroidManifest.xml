<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tarsbir.mysqltutorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AskQuestion"
            android:label="@string/Ask_question"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TutorialActivity"
            android:label="@string/About_tutor"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MachineExerciseOneActivity"
            android:label="MachineExcerciseOne"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>